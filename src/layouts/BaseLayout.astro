---
import BaseHead from '../components/BaseHead.astro';

export interface Props {
  title: string;
  description: string;
  permalink: string;
  current?: string;
  heroImage?: string
}

const { title, description, permalink, current, heroImage } = Astro.props;
---
<html lang="en">
<head>
  <BaseHead 
    title={title} 
    description={description} 
    permalink={permalink} 
    current={current}
    heroImage={heroImage}
  />
</head>
<body>
  <slot />
</body>
</html>

<style>
  
</style>
